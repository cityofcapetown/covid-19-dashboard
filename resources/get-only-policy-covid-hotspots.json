{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "s3:GetObject",
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Effect": "Allow",
      "Resource": [
        "arn:aws:s3:::covid/*-hotspots.html",
        "arn:aws:s3:::covid/assets/*",
        "arn:aws:s3:::covid/data/public/*",
        "arn:aws:s3:::covid/widgets/public/*",
        "arn:aws:s3:::covid/widgets/private/libdir/*",
        "arn:aws:s3:::covid/widgets/private/plotly.min.js",
        "arn:aws:s3:::covid/widgets/private/case_count_maps/*",
        "arn:aws:s3:::covid/widgets/private/covid19_stat_table*",
        "arn:aws:s3:::covid/widgets/private/*.js",
        "arn:aws:s3:::covid/widgets/private/*.css",
        "arn:aws:s3:::covid/widgets/private/ct_*",
        "arn:aws:s3:::covid/widgets/private/cct_*",
        "arn:aws:s3:::covid/widgets/private/wc_model_*",
        "arn:aws:s3:::covid/widgets/private/latest_values.json"
      ],
